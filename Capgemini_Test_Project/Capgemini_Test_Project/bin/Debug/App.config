<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>
  <appSettings>
    <!--Determines which environment to work values supported: Production,QA -->
    <add key="WorkingEnvironment" value="QA" />
    <!--Base URL of the application-->
    <add key="Base_URL" value="http://www.google.com" />
    <!--Determines what browser to use when doing the test: 0-IE,1-Firefox,2-Chrome, 3-Safari-->
    <add key="BrowserType" value="2" />
    <!--Capture screenshot when test step gets failed or for all executed steps-->
    <add key="CapturescreenshotforAllsteps" value="0" />
    <!--Time out in Seconds.-->
    <add key="WaitTimeOut" value="60" />
    <!--Determines what browser to use when doing the test: 0-IE,1-Firefox,2-Chrome, 3-Safari-->
    <add key="BrowserType" value="2" />
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
<specFlow>
    
  <stepAssemblies>
      <!-- This attribute is required in order to use StepArgument Transformation as described here; 
    https://github.com/marcusoftnet/SpecFlow.Assist.Dynamic/wiki/Step-argument-transformations  -->
      <stepAssembly assembly="SpecFlow.Assist.Dynamic" />
    </stepAssemblies><!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config --><!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config --><unitTestProvider name="NUnit" /><plugins>
      <add name="SpecFlow.Plus.Excel" type="Generator" />
    </plugins></specFlow>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="TechTalk.SpecFlow" publicKeyToken="0778194805d6db41" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.3.2.0" newVersion="2.3.2.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>